{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "dist\\test",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      // Serves index.html for requests to files or directories that do not exist
      "source": "**",
      "destination": "/index.html"
    }]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
